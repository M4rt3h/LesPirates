package pirates;

public class CarteRegenHP extends Carte {

	int regen = 2;

	public CarteRegenHP() {
		setDescription("Tu bois du rhum dans le crâne de l'ancien capitaine");
		setNomCarte("Regeneration de vie");

	}

	@Override
	public String carteToString() {
		String affichageCarte = nomCarte + "\n" + description + "\n" + "Tu récupères " + String.valueOf(regen) + " PV\n";
		return affichageCarte;
	}

}
