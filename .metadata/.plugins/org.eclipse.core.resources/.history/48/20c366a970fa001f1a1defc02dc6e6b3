package pirates;

public class CarteRegenHP extends Carte {

	int regen = 2;

	private int getRegen() {
		return regen;
	}

	public CarteRegenHP() {
		setDescription("Tu bois du rhum dans le crâne d'un de tes défunts ennemi");
		setNomCarte("Regeneration de vie");

	}

	@Override
	public String carteToString() {
		String affichageCarte = getNomCarte() + "\n" + "Tu récupères " + String.valueOf(getRegen()) + " PV\n";
		return affichageCarte;
	}

}
